@model Messenger.Models.AddFreindViewModel

@{
    ViewBag.Title = "EditFriend";
}

<div class="form-1">
    <h1 class="h1_1">ویرایش مخاطب</h1>

    <div class="div1">
        <label>موبایل :</label>
        <input type="text" maxlength="11" ng-model="PhoneNumber" ng-init="PhoneNumber='@Model.PhoneNumber'" />
    </div>
    <div class="div1">
        <label>نام :</label>
        <input type="text" maxlength="50" ng-model="FullName" ng-init="FullName='@Model.FullName'" />
    </div>
    <div class="div1">
        <p id="sss"></p>
        <input type="button" value="ویرایش مخاطب" class="input-submit AddFriend-submit" ng-click="editFriend(@Model.FriendId)">
    </div>
</div>

@section Script{
    <script src="~/Content/Scripts/User_Friend.js"></script>
}


